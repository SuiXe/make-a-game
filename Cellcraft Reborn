<script>
var gmselect = 0;
var gm = "cellcraft.io:4041";
var img = "craft";
var con = "NA";
function getGameMode(a) {
if (a == "add") { 
    gmselect++; 
} else if (a == "remove") {
 gmselect--; 
} else { 
    console.log("ERROR"); 
}
console.log(gmselect);
console.log(gmselect);
console.log(gmselect);

if (gmselect < 0) { 
    gmselect = 10; //the last to ze right
} else if (gmselect > 10) {
 gmselect = 0; 
}
if (gmselect == 0) { 
    gm = "cellcraft.io:4041"; 
    img = "craft3";
    var con = "NA";
} else if (gmselect == 1) {
 gm = "108.61.210.196:4041";
 img = "scania2";
 con = "EU";
} else if (gmselect == 2) {
 gm = "45.32.186.119:4041";
